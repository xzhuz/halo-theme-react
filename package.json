{
  "name": "ssr-with-js",
  "version": "2.3.3",
  "dependencies": {
    "axios": "^0.21.0",
    "disqusjs": "^1.3.0",
    "egg": "^2.21.0",
    "egg-scripts": "^2.13.0",
    "egg-static": "^2.2.0",
    "highlight.js": "^10.3.2",
    "isotope-layout": "^3.0.6",
    "jsdom": "^16.4.0",
    "marked": "^1.2.3",
    "prop-types": "^15.7.2",
    "qs": "^6.9.4",
    "react": "16.8.3",
    "react-animations": "^1.0.0",
    "react-dom": "16.8.3",
    "react-helmet": "^6.1.0",
    "react-images": "^1.1.7",
